{
  "name": "portfolio-site",
  "version": "1.0.0",
  "description": "My portfolio website",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel watch:sass devserver",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirankunigiri/kirankunigiri.github.io.git"
  },
  "author": "Kiran Kunigiri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kirankunigiri/kirankunigiri.github.io/issues"
  },
  "homepage": "https://github.com/kirankunigiri/kirankunigiri.github.io#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0"
  },
  "dependencies": {
    "atvimg": "^0.0.4"
  }
}
